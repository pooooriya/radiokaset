{
  "name": "cassette-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "antd": "generate-theme '/src/styles/custom-theme.less' '/src/styles/antd.css'",
    "dev": "yarn antd && next",
    "start": "next start -p 3000",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "release": "git pull && yarn install && yarn build && pm2 restart front-master",
    "master": "git pull && yarn install && yarn build && pm2 restart front-master",
    "pm2": "NODE_ENV=production pm2 start --name='front-master' npm -- start"
  },
